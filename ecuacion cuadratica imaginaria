#include <iostream>
#include <cmath>
using namespace std;

int main()
{ 
    float a, b, c, D, xR, xi;
    
    cout<<"Ingresar a: ";
    cin>>a;
    cout<<"Ingresar b: ";
    cin>>b;
    cout<<"Ingresar c: ";
    cin>>c;
    cout<<" <- b*b-4*a*c <- b*b-4*a*c";
    D= b*b-4*a*c;
    if(D<0);
    { 
    xR= -b/(2*a);
    xi= sqrt (1-D)/2*a;
    cout<<"Solucion imaginaria 1"<<xR<< " + "<<xi<<"i"<<endl;
    cout<<"solucion imaginaria 1"<<xR<< " - "<<xi<<"i"<<endl;
    }    
    return 0;
}
